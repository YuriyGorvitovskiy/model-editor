        "jest": "25.4.0",
        "prettier": "2.0.5",
        "ts-jest": "25.4.0",
        "ts-loader": "7.0.1",
        "ts-node": "8.9.1",
        "tslint": "6.1.1",
        "tslint-config-prettier": "1.18.0",
        "typedoc": "0.17.4",
        "typescript": "3.8.3",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11"
    },
    "dependencies": {
