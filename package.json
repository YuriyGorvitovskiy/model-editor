        "jest": "23.6.0",
        "prettier": "1.15.3",
        "ts-jest": "23.10.5",
        "ts-loader": "5.3.3",
        "ts-node": "7.0.1",
        "tslint": "5.12.1",
        "tslint-config-prettier": "1.17.0",
        "typedoc": "0.14.2",
        "typescript": "3.2.2",
        "webpack": "4.28.4",
        "webpack-cli": "3.2.1"
    },
    "dependencies": {
        "@material-ui/core": "3.9.3",
