{
    "name": "model-editor",
    "version": "0.1.0",
    "description": "",
    "main": "build/app/app-bundle.js",
    "scripts": {
        "upgrade-dep": "npx npm-check-updates -u && npm install",
        "compile": "tsc",
        "copy": "copyfiles -u 2 src/window/*.html build/window/",
        "format": "prettier --write \"src/**/*.ts\" .prettierrc jest.config.js package.json tsconfig.json tslint.json",
        "lint": "tslint -p tsconfig.json",
        "test": "jest --coverage",
        "webpack": "webpack --config webpack.app.js && webpack --config webpack.window.js ",
        "build": "npm run format && npm run compile",
        "check": "npm run format && npm run compile && npm run lint && npm run test",
        "package": "npm run format && npm run lint && npm run copy && npm run webpack",
        "start": "electron ."
    },
    "author": "Yuriy Gorvitovskiy",
    "license": "ISC",
    "dependencies": {
        "@material-ui/core": "4.9.11",
        "@material-ui/icons": "4.9.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "state-glue": "file:../state-glue",
        "typeface-roboto": "0.0.75"
    },
    "devDependencies": {
        "@types/jest": "25.2.1",
        "@types/react": "16.9.34",
        "@types/react-dom": "16.9.6",
        "copyfiles": "2.2.0",
        "css-loader": "3.5.3",
        "electron": "8.2.3",
        "extract-loader": "^5.0.1",
        "file-loader": "6.0.0",
        "jest": "25.4.0",
        "postcss-loader": "^3.0.0",
        "prettier": "2.0.5",
        "style-loader": "1.2.0",
        "ts-jest": "25.4.0",
        "ts-loader": "7.0.1",
        "ts-node": "8.9.1",
        "tslint": "6.1.1",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.8.3",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11"
    }
}
